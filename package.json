{
  "name": "sx-launcher",
  "version": "0.2.0",
  "description": "",
  "main": "./dist/main.js",
  "build": {
    "productName": "Sx Launcher",
    "appId": "ml.xecus.sxl",
    "copyright": "Xecus",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "icon": "./static/src/assets/icon.ico",
      "target": [
        {
          "target": "dir"
        }
      ]
    }
  },
  "scripts": {
    "build-all": "npm run build-main && npm run build-static",
    "build-static": "cd static && npm test",
    "build-main": "tsc",
    "clear": "rimraf ./dist",
    "start": "npm run clear && npm run build-all && electron-builder",
    "test": "npm run clear && npm run build-all && electron ."
  },
  "author": "Xs!",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^12.9.0",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
